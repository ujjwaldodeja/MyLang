print(5 / 6);
print(8 / 2);
print(9 / -1);
print(-9 / 2);
print(0 / 10);
print(9999 / 1);
print(9/9);

int x, y[3];
x = -200;
y = [0, -40, -350];
print(x / y[1]);
print(y[2] / x);

print(8 / 0);

// Expected output:
//Sprockell 0 says 0
//Sprockell 0 says 4
//Sprockell 0 says -9
//Sprockell 0 says -4
//Sprockell 0 says 0
//Sprockell 0 says 9999
//Sprockell 0 says 1
//Sprockell 0 says 5
//Sprockell 0 says 1
//Error: Cannot divide by 0
